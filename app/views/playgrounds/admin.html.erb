<!DOCTYPE html>
<html>
	<head>
		<link href="bootstrap.css" rel="stylesheet">
		<%= stylesheet_link_tag "application", :media => "all" %>
		<!-- <link rel="stylesheet" type="text/css" href="anytime.css" /> -->
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
		<script src="js/bootstrap.min.js"></script>

	</head>
	<body>

		<div class="col-md-12">
			<h3>Administration</h3>

			<div class="panel with-nav-tabs panel-primary">

				<div class="panel-heading">
					<ul class="nav nav-tabs">
						<li class="active">
							<a href="#tab1primary" data-toggle="tab">Pending</a>
						</li>
						<li>
							<a href="#tab2primary" data-toggle="tab">Activated</a>
						</li>
						<li>
							<a href="#tab3primary" data-toggle="tab">Deleted</a>
						</li>

					</ul>
				</div>

				<div class="panel-body">
					<div class="tab-content">
						<div class="tab-pane fade in active" id="tab1primary">

							<%= render 'pendingpartial' %>
						</div>

						<div class="tab-pane fade" id="tab2primary">
							<%= render 'activatedpartial'%>
						</div>
						<div class="tab-pane fade" id="tab3primary">
							<%= render 'deletedpartial'%>
						</div>

					</div>
				</div>
			</div>
		</div>
	</body>
</html>

<script>
	function update_house(id,status){
$.ajax({
type: "get",
url: "/playgrounds/update_house_status",
data: {
"id": id,
"status": status
},
dataType : 'script',
iframe: 'true',
cache: false,
success: function(data){
}
});

}

function update_location(lat,log,p_id){

$.ajax({
type: "get",
url: "/playgrounds",
data: {
"lat": lat,
"log": log,
"id":p_id
},
dataType : 'script',
iframe: 'true',
cache: false,
success: function(data){
}
});

// Invoke it
// createMarkerWithInfoWindow();
}

</script>